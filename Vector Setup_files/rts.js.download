$(document).ready(function () {
  $(".vec-env-select-btn").click(function () {
    // Check network status when button is clicked
    if (navigator.onLine) {
      console.log("online");
    } else {
      console.log("offline");
    }

    // Handle stack selection
    var selectedStack = $("#envOptions").val();
    handleStackSelection(selectedStack);

    // Ensure offline check after handling stack selection
    checkForOfflineStatus();
  });

  $("#btnDiscoverVector").click(function () {
    console.log("hbbi");
  });
});

// Function to check and log offline status
function checkForOfflineStatus() {
  if (!navigator.onLine) {
    console.log("offline");
  }
}
